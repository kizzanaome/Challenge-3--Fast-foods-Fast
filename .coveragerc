# .coveragerc to control coverage.py
[report]
# Regexes for lines to exclude from consideration
omit =
    */python?.?/*
    */site-packages/nose/*
    */__pycache__/*
    *.pyc
    */venv/*
    */home/travis/virtualenv/*
    */.pytest_cache/*
    */renamed/*
    */ignored/*
    */.vscode/*
    */tests/*
    
